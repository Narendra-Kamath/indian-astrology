{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "rules": {
        "linebreak-style": "off",
        "indent": "off",
        "strict": "off",
        "prefer-const": "off",
        "no-mixed-operators": "off",
        "no-param-reassign": "off",
        "no-plusplus": "off",
        "eol-last": "off",
        "no-unused-vars": "off"
    }
}